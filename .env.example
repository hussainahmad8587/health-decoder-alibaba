# Mode switches
HD_VISION_BACKEND=local           # local | alibaba_imm
HD_STORAGE_BACKEND=local          # local | alibaba_oss

# --- Alibaba Cloud (if using cloud backends) ---
ALIBABA_REGION=cn-shanghai

# OSS
ALIBABA_OSS_ENDPOINT=https://oss-cn-shanghai.aliyuncs.com
ALIBABA_OSS_BUCKET=your-bucket
ALIBABA_ACCESS_KEY_ID=xxx
ALIBABA_ACCESS_KEY_SECRET=xxx

# IMM (Face detection) - depending on your IMM setup (project/endpoint/credentials)
ALIBABA_IMM_ENDPOINT=imm.cn-shanghai.aliyuncs.com
ALIBABA_IMM_PROJECT=your-imm-project

# PAI EAS (optional)
PAI_EAS_ENDPOINT=https://xxxx.cn-shanghai.pai-eas.aliyuncs.com/api/predict/your_service
PAI_EAS_TOKEN=your_token_or_auth
